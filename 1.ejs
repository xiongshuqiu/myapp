function getNavItems(role) {
  const navItems = [
      { id: 'dashboard-link', visible: true },
      { id: 'userManagement-link', visible: true },
  ];

  switch (role) {
      case 'family':
          navItems[1].visible = false; // 隐藏 dashboard
          break;
      case 'medical':
          navItems[1].visible = false; // 隐藏 User Management
          break;
      case 'admin':
          navItems[1].visible = true; // 显示 User Management
          break;
      default:
          // 默认行为（如果有）
          break;
  }

  return navItems;
}
