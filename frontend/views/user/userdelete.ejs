    <title>chinaz</title>
    <!-- Bootstrap Styles使用Bootstrap框架样式-->
    <!--Bootstrap Styes是流行的前端框架-->

    <link href="/assets/css/bootstrap.css" rel="stylesheet" />
    <!--href 属性指定了要引入的 CSS 文件的路径-->
    <!-- FontAwesome Styles图标样式-->
    <!--FontAwesome是一个流行的图标库-->

    <link href="/assets/css/font-awesome.css" rel="stylesheet" />
    <!--rel 的值通常是 "stylesheet"，表示这是一个样式表。-->
    <!-- Morris Chart Styles图表样式-->
    <link href="/assets/js/morris/morris-0.4.3.min.css" rel="stylesheet" />
    <!-- Custom Styles普通样式-->
    <link href="/assets/css/custom-styles.css" rel="stylesheet" />
    <!-- Google Fonts字体-->
    <link
      href="http://fonts.useso.com/css?family=Open+Sans"
      rel="stylesheet"
      type="text/css"
    />
    <!-- 轻量级图表样式-->
    <link rel="stylesheet" href="/assets/js/Lightweight-Chart/cssCharts.css" />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css"
    />
